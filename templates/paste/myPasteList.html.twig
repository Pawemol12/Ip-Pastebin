{% extends 'base.html.twig' %}

{% set title='pastebin.myPasteList'|trans %}
{% set page=constant('App\\Enum\\PagesEnum::PASTEBIN_PAGE') %}

{% block body %}
    <div class="row">
        <div class="col">
            <h2>{{ title }}</h2>
            <hr/>
            <div id="MyPastesSearchFormWrapper">
                {% include 'paste/form/myPastesSearchForm.html.twig' %}
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col">
            {% include 'snippets/alertContainer.html.twig' %}
        </div>
    </div>

    <div class="row">
        <div class="col">
            <div id="MyPastesTableWrapper">
                {% include 'paste/table/myPastesTableWrapper.html.twig' %}
            </div>
        </div>
    </div>

    <div id="ModalContainer"></div>
{% endblock %}